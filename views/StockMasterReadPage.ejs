<!DOCTYPE html>
<html>
<head lang="en">
<% include head %>
</head>
<body>
<% include side-nav %>
<div id="main">
<% include header-nav %>
<div class="container-fluid">
<section id="main-content">

    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true"><h4><%=resource.Heading%></h4></a>
      </li>
    </ul>
    
    <div class="tab-content" id="myTabContent">

        <div class="tab-pane fade in active" id="home" role="tabpanel" aria-labelledby="home-tab">	
            <div style="width:40%">
            <form class="form-horizontal">
                <table class="table table-striped">
                    <tr class="heading-tr">
                        <th> <%=resource.Heading%></th>
                        <th></th>
                    </tr>
                    <tr>
                        <td><%=resource.Id.label%></td>
                        <td><%=record.id%></td>						
                    </tr><tr>
                        <td><%=resource.Code.label%></td>
                        <td><%=record.code%></td>						
                    </tr><tr>
                        <td><%=resource.Units.label%></td>
                        <td><%=record.units%></td>						
                    </tr><tr>
                        <td><%=resource.Mbflag.label%></td>
                        <td><%=record.mbflag%></td>						
                    </tr><tr>
                        <td><%=resource.Lastcurcostdate.label%></td>
                        <td><%=dateFormat(record.lastcurcostdate,'mmm-dd-yyyy HH:MM:ss')%></td>						
                    </tr><tr>
                        <td><%=resource.Actualcost.label%></td>
                        <td><%=record.actualcost%></td>						
                    </tr><tr>
                        <td><%=resource.Lastcost.label%></td>
                        <td><%=record.lastcost%></td>						
                    </tr><tr>
                        <td><%=resource.Materialcost.label%></td>
                        <td><%=record.materialcost%></td>						
                    </tr><tr>
                        <td><%=resource.Labourcost.label%></td>
                        <td><%=record.labourcost%></td>						
                    </tr><tr>
                        <td><%=resource.Overheadcost.label%></td>
                        <td><%=record.overheadcost%></td>						
                    </tr><tr>
                        <td><%=resource.Lowestlevel.label%></td>
                        <td><%=record.lowestlevel%></td>						
                    </tr><tr>
                        <td><%=resource.Discontinued.label%></td>
                        <td><%=record.discontinued%></td>						
                    </tr><tr>
                        <td><%=resource.Controlled.label%></td>
                        <td><%=record.controlled%></td>						
                    </tr><tr>
                        <td><%=resource.Eoq.label%></td>
                        <td><%=record.eoq%></td>						
                    </tr><tr>
                        <td><%=resource.Volume.label%></td>
                        <td><%=record.volume%></td>						
                    </tr><tr>
                        <td><%=resource.Kgs.label%></td>
                        <td><%=record.kgs%></td>						
                    </tr><tr>
                        <td><%=resource.Barcode.label%></td>
                        <td><%=record.barcode%></td>						
                    </tr><tr>
                        <td><%=resource.Discountcategory.label%></td>
                        <td><%=record.discountcategory%></td>						
                    </tr><tr>
                        <td><%=resource.Taxcatid.label%></td>
                        <td><%=record.taxcatid%></td>						
                    </tr><tr>
                        <td><%=resource.Serialised.label%></td>
                        <td><%=record.serialised%></td>						
                    </tr><tr>
                        <td><%=resource.Appendfile.label%></td>
                        <td><%=record.appendfile%></td>						
                    </tr><tr>
                        <td><%=resource.Perishable.label%></td>
                        <td><%=record.perishable%></td>						
                    </tr><tr>
                        <td><%=resource.Decimalplaces.label%></td>
                        <td><%=record.decimalplaces%></td>						
                    </tr><tr>
                        <td><%=resource.Nextserialno.label%></td>
                        <td><%=record.nextserialno%></td>						
                    </tr><tr>
                        <td><%=resource.Pansize.label%></td>
                        <td><%=record.pansize%></td>						
                    </tr><tr>
                        <td><%=resource.Shrinkfactor.label%></td>
                        <td><%=record.shrinkfactor%></td>						
                    </tr>
                
                </table>
                <% if(session.user_acl=='ROLE_ADMIN') { %>
                <button type="submit" class="btn btn-default"><a href="/writeStockMaster?id=<%=record.id%>">Edit</a></button>
                <button type="submit" class="btn btn-default"><a href="/deleteStockMaster?id=<%=record.id%>">Delete</a></button>
                <% } %>
                <button type="button" class="btn btn-default" onclick="javascript:window.history.back()">Back</button>
            </form>
            </div>
        </div>
        
    </div>

</section>
</div>
</div>
<% include footer %>
</body>
</html>
