<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="newAccountGroupMapper">			
	
	<insert id="newAccountGroup">
		insert into account_group(id,name,parent,description,acc_section_id) 
		values (#{id},#{name},#{parent},#{description},#{accSectionId});
	</insert>

</mapper>
